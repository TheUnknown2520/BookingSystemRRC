@page
@model BookingSystemRRC.Pages.InternallySystem.EmployeeCalendarModel
@{


    <div class="calendar">

        <header>

            <div>
                <nav>
                    <button> Create Customer </button>
                    <a class="CalendarBTN" type="button" asp-page="/BookingRRC/CreateBooking"> Create Booking </a>
                    <button> Check in </button>
                </nav>
            </div>

            <div class="calendarTitle">
                <h1 class="header"><span></span><strong>03 May – 09 May</strong> 2021</h1>
            </div>

        </header>

        <div class="outer">

            

            <table>
                <thead>
                    <tr>
                        <th class="headcol"></th>
                        <th>Mon, 03</th>
                        <th>Tue, 04</th>
                        <th>Wed, 05</th>
                        <th>Thu, 06</th>
                        <th>Fri, 07</th>
                        <th>Sat, 08</th>
                        <th>Sun, 09</th>
                    </tr>
                </thead>
            </table>

            <div class="wrap">
                <table>

                    <tbody runat="server" id="calendarTableBody">

                        <tr>
                            <td class="headcol"> 09:00 </td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                        </tr>
                    </tbody>

                </table>
            </div>
        </div>
    </div>


}



